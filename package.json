{
  "name": "TeamOB",
  "version": "1.0.45",
  "description": "TeamOB installer",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/teamobtool/ob.git"
  },
  "build": {
    "asar": false,
    "win": {
      "icon": "./assets/app-icon/win/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "zip",
      "icon": "./assets/app-icon/png/32x32.png.png",
      "executableName":"TeamOB"
    },
    "extraFiles": [
      "log/*"
    ],
    "nsis": {
      "guid": "eb1a0fbb-fc70-428e-97f1-fa7080894809",
      "include": "installer.nsh",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "warningsAsErrors": false,
      "createStartMenuShortcut": true
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder build --win --ia32 --publish never",
    "linux": "electron-builder build --linux --publish never",
    "deploy": "electron-builder build --win --publish always"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.1",
    "electron-builder": "^20.40.2"
  },
  "dependencies": {
    "aes-ecb": "^1.3.10",
    "chokidar": "^3.4.2",
    "electron-localshortcut": "^3.2.1",
    "electron-regedit": "^2.0.0",
    "electron-updater": "^4.0.6",
    "http-proxy": "^1.18.1",
    "socket.io-client": "^2.3.0",
    "tcp-port-used": "^1.0.1"
  }
}
